model User {
  id        String   @id @default(uuid())
  name      String
  password  String
  phoneNo   String   @unique @db.VarChar(10)
  email     String   @unique
  bio       String
  isBlocked Boolean  @default(false)
  isDeleted Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
